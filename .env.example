# IPhey Backend API Configuration
# Copy this file to .env and fill in your actual API keys

# Required: IP Geolocation Provider
IPINFO_TOKEN=your_ipinfo_token_here

# Required: Cloudflare Radar API
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_RADAR_TOKEN=your_cloudflare_radar_token

# Optional: IP Reputation Services
ABUSEIPDB_API_KEY=your_abuseipdb_key_here

# Server Configuration
PORT=4310
NODE_ENV=development
LOG_LEVEL=info

# Caching
CACHE_TTL_MS=300000
CACHE_MAX_ITEMS=500
CLIENT_TIMEOUT_MS=2500

# Creepjs Assets
CREEPJS_ASSETS_PATH=../creepjs/dist
